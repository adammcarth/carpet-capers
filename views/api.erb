{<% @rooms.each do |room| %>
  "<%= room.number %>": {
    "area": <%= room.area %>,
    "broadloom": <%= room.broadloom %>,
    "carpet_type": "<%= room.carpet_type %>",
    "cost": <%= room.cost %><% if room.discount %>,
    "discount": <%= room.discount %><% end %>
  }<% unless room == @rooms.last %>,<% end %>
<% end %>}